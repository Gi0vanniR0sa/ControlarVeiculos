<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.WithdrawVehicle">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" onAction="#goToMenu" text="Menu">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#goToMenu" text="Menu" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <Label layoutX="318.0" layoutY="66.0" prefHeight="27.0" prefWidth="158.0" text="Retirada do Veículo">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
            <Label layoutX="100.0" layoutY="161.0" text="Motorista :">
               <font>
                  <Font name="Ricardo-Regular" size="14.0" />
               </font></Label>
            <Label layoutX="322.0" layoutY="160.0" prefHeight="20.0" prefWidth="61.0" text="Veículo :">
               <font>
                  <Font size="14.0" />
               </font></Label>
            <Label layoutX="538.0" layoutY="161.0" prefHeight="18.0" prefWidth="76.0" text="Retirada :">
               <font>
                  <Font name="Ricardo-Regular" size="14.0" />
               </font></Label>
            <Button layoutX="300.0" layoutY="350.0" mnemonicParsing="false" onAction="#goToMenu" text="Cancelar">
               <font>
                  <Font name="Ricardo-Regular" size="14.0" />
               </font></Button>
            <Button layoutX="406.0" layoutY="350.0" mnemonicParsing="false" onAction="#withdrawVehicle" prefHeight="28.0" prefWidth="76.0" text="Gravar">
               <font>
                  <Font name="Ricardo-Regular" size="14.0" />
               </font></Button>
            <ComboBox fx:id="drivers" layoutX="100.0" layoutY="190.0" prefWidth="150.0" />
            <ComboBox fx:id="vehicles" layoutX="322.0" layoutY="190.0" prefWidth="150.0" />
            <DatePicker fx:id="dateWithdraw" layoutX="538.0" layoutY="190.0" />
         </children>
    </AnchorPane>
  </children>
</VBox>
