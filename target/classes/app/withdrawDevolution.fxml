<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.WithdrawDevolution">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" onAction="#goToMenu" text="Menu">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#goToMenu" text="Menu" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label layoutX="297.0" layoutY="78.0" text="Devolução de Veículo">
          <font>
            <Font name="Ricardo-Regular" size="18.0" />
          </font>
        </Label>
        <Label layoutX="200.0" layoutY="157.0" prefHeight="18.0" prefWidth="157.0" text="Veiculos em uso :">
               <font>
                  <Font size="14.0" />
               </font></Label>
        <Label layoutX="434.0" layoutY="158.0" prefHeight="18.0" prefWidth="172.0" text="Data de devoluçao :">
               <font>
                  <Font name="Ricardo-Regular" size="14.0" />
               </font></Label>
        <Button layoutX="267.0" layoutY="343.0" mnemonicParsing="false" onAction="#goToMenu" prefHeight="28.0" prefWidth="78.0" text="Cancelar">
               <font>
                  <Font name="Ricardo-Regular" size="14.0" />
               </font></Button>
        <Button layoutX="429.0" layoutY="343.0" mnemonicParsing="false" onAction="#withdrawDevolution" prefHeight="28.0" prefWidth="78.0" text="Gravar">
               <font>
                  <Font name="Ricardo-Regular" size="14.0" />
               </font></Button>
        <ComboBox fx:id="withdraws" layoutX="188.0" layoutY="194.0" prefHeight="26.0" prefWidth="182.0" />
        <DatePicker fx:id="dateDevolution" layoutX="429.0" layoutY="194.0" prefHeight="28.0" prefWidth="182.0" promptText="Escolha a data aqui." />
      </children>
    </AnchorPane>
  </children>
</VBox>
