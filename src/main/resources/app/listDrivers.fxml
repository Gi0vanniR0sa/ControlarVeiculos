      <menus>
        <Menu mnemonicParsing="false" onAction="#goToMenu" text="Menu">
          <items>
            <MenuItem mnemonicParsing="false" onAction="#goToMenu" text="Valtar ao menu" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <Label contentDisplay="CENTER" layoutX="299.0" layoutY="79.0" prefHeight="27.0" prefWidth="201.0" text="Listagem de motoristas" textAlignment="CENTER" textOverrun="CENTER_ELLIPSIS">
          <font>
            <Font name="Ricardo-Regular" size="18.0" />
          </font>
        </Label>
        <ListView fx:id="drivers" layoutX="45.0" layoutY="182.0" prefHeight="221.0" prefWidth="710.0">
               <opaqueInsets>
                  <Insets />
